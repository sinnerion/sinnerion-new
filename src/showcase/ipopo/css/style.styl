.menu-block-wrap
  float: left
  position: relative
  width: 19.5%
  height: 123px
  margin-left: 0.9%
.menu-block
  position: absolute
  top: -12px
  left: 0px
  z-index: 0
  width: 100%
  &__wrapper
    transform: translate3d(0, 0, 0) rotate(2deg)
    background: #804fa9
    border-radius: 0 0 15px 10px
    width: 100%
    height: 103px
    position: relative
    z-index: 0
    transition: all .2s ease-in-out
  &__in
    transform: translate3d(0, 0, 0) rotate(-1deg)
    position: absolute
    display: block
    top: 3px
    left: 2px
    z-index: 1
    width: 94%
    height: 90px;
    border-radius: 0 0 10px 15px
    background: #8d5bb4
    //opacity .3
    transition: all 0.2s ease-in-out
    &
      .menu-block__content
        position: absolute
        bottom: 10px
        left: 15%
        transform: translate3d(0, 0, 0) rotate(2deg)
        & span
          display: inline-block
          vertical-align: middle
          font-family: bloggersans-bold, Tahoma,Arial, sans-serif
          font-weight: 700
          font-size: 18px
          max-width: 40%
          color: #fff
          text-transform: uppercase
          text-shadow: 2px 2px 0 rgba(0,0,0,.16)
          transition all .2s ease-in-out

  &:hover
    .menu-block__wrapper
      height: 113px
    //background-color: main-color
    .menu-block__in
      height: 100px
      span
        text-shadow: 3px 3px 5px rgba(0,0,0,.4)

  &__icon
    display: inline-block
    vertical-align: middle
    width: 64px
    margin-left: -11px
    margin-right: 10px
    background-size: contain

