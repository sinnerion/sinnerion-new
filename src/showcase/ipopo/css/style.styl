      outline none
    & + input
      float: right;
      border-left: 1px solid neutral-color;
      padding-left: 10px;
      width: 30px;
      display: inline-block;
      background: url(/img/svg/search.svg) no-repeat 100% 50%;

.filter-sort
  padding: 20px 20px 30px
  text-align: right
  color: neutral-color
  .input-group
    a
