@import '_vars'
@import '_fonts'
@import '_reset'
a
  color inherit


h1, h2, h3, h4, h5
  font-size inherit

body
  color main-color
  font main-font

.text-center
  text-align: center

.my-btn
  padding: 10px 20px
  border-radius: 30px
  display: inline-block
  background-image: -moz-linear-gradient(90deg, rgb(141, 91, 180) 0%, rgb(173, 105, 226) 100%);
  background-image: -webkit-linear-gradient(90deg, rgb(141, 91, 180) 0%, rgb(173, 105, 226) 100%);
  background-image: -ms-linear-gradient(90deg, rgb(141, 91, 180) 0%, rgb(173, 105, 226) 100%);
  box-shadow: 0 8px 9px 0 rgba(98, 36, 147, 0.85), 0px 5px 0px rgba(98, 36, 147, 1);
  position: relative;
  transition: top .25s ease-in-out;
  color: #fff
  font-family bloggersans-bold, arial, sans-serif
  text-transform: uppercase
  margin-bottom: 10px
  &:active
    top: 5px;
    box-shadow: 0 0 4px 0 rgba(98, 36, 147, 0.85), 0px 0px 0px rgba(98, 36, 147, 1);

  &:hover,
  &:visited,
  &:focus
    text-decoration: none
    outline: none
    color: #fff

.btn-gift
  padding-left 56px
  position: relative
  font-size 1.5rem
  &:after
    content: ''
    width: 25px
    position: absolute
    background: url(../img/svg/gift-purplewhite.svg) no-repeat 50% 50%
    height: 25px
    background-position: 0 0
    background-size: contain
    top: 7px
    left: 18px

.prod-name,
.price
  margin: 10px 0

.prod-name
  font-size: 20px
  height: 45px
  overflow: hidden

.price
  font-family ptsans-regular, arial, sans-serif
  color: accent-color
  font-size 2.4rem

#menu
  padding: 20px 0

section#market, #product
  background: #f4f4f4
  padding: 20px 0

#resp
  position: absolute
  background: #fff
  top: 100%
  padding: 10px 5px
  left: 0
  border: 1px solid
  border-radius: 15px
  width: 200%
  z-index 15
  a
    padding 5px 10px

.box
  background-color: #fff
  box-shadow 0 0 5px rgba(0, 0, 0, .1)
  border-top 1px solid #cfcfcf
  margin-bottom: 20px
  & > div
    border-bottom 1px solid #f4f4f4
    padding: 10px

.catalog
  li
    border-bottom: 1px solid #eee
    &:last-child
      border-bottom: none
  a
    display: inline-block
    padding: 7px 5px
    width: 100%

//&:hover
//  background lighten(main-color,80%)
.menu-products
  display none

.cat-trigger
  text-align: center
  width: 100%

.catalogue
  position: relative
  top: 10px
  #catalogue
    position: absolute
    z-index 15
    //padding: 10px
    padding-right: 0
    border 1px solid main-color
    border-radius 4px
    background #fff
    width: 190px
    li
      &.active
        background lighten(main-color, 80%)

        & > ul,
        & > .menu-products
          display inline-block
          position absolute
          top: 0
          white-space nowrap
          //padding: 10px 5px 0
          left: 100%
          border 1px solid main-color
          border-radius 4px
          background #fff
          min-height 100%
          min-width 190px
      a
        padding: 7px 10px
        padding-right: 20px
        position: relative
        &.childs
          &:after
            content: ''
            display: block
            width: 10px
            font-size: 10px
            height: 10px
            border-top: 5px solid transparent
            border-left: 5px solid
            border-bottom: 5px solid transparent
            position: absolute
            top: 50%
            right: 0
            transform: translate(0, -50%)

#slider
  margin: 20px auto;
  width: 90%

.ui-slider
  position: relative
  text-align: left
  margin: 10px 0

.ui-slider .ui-slider-handle
  position: absolute
  z-index: 2
  width: 1.2em
  height: 1.2em
  cursor: default
  -ms-touch-action: none
  touch-action: none

.ui-slider .ui-slider-range
  position: absolute
  z-index: 1
  font-size: .7em
  display: block
  border: 0
  height: 2px
  background-position: 0 0
  background-color: main-color

.ui-slider-horizontal
  height: 2px
  background-color: neutral-color
  border-radius: 2px

.ui-slider-horizontal .ui-slider-handle
  background-image: -moz-linear-gradient(90deg, #8d5bb4 0%, #ad69e2 100%);
  background-image: -webkit-linear-gradient(90deg, #8d5bb4 0%, #ad69e2 100%);
  background-image: -ms-linear-gradient(90deg, #8d5bb4 0%, #ad69e2 100%);
  box-shadow: 0 8px 9px 0 rgba(98, 36, 147, 0.85), 0px 5px 0px #622493;
  border-radius 50%
  top: -10px
  outline none
  margin-left: -.6em
  .ui-slider-range
    top: 0
    height: 5px
  .ui-slider-range-min
    left: 0
  .ui-slider-range-max
    right: 0

.price-in
  width: 25%
  display: inline-block
  border 1px solid neutral-color
  padding: 3px
  box-shadow inset 1px 2px 0 rgba(0, 0, 0, .15)
  &:focus
    outline none

#price
  padding: 0
  border-radius: 5px
  font-weight: bold
  font-size 2rem
  width: 30px
  border: none
  outline: none

.shop-content, .product-content
  background-color: #fff
  box-shadow 0 0 5px rgba(0, 0, 0, .1)
  border-top: 2px solid main-color
  margin-bottom: 20px

.no-padding
  padding: 0

.input-group
  background transparent
  padding: 5px 10px
  display: block
  border-radius: 20px
  border 1px solid neutral-color
  & > *:not(form)
